{
  "name": "@nimpl/context",
  "version": "1.0.0",
  "description": "Implementation of server contexts in React Server Components without switching to SSR",
  "files": [
    "**/*.js",
    "**/*.d.ts"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/cjs/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "default": "./dist/cjs/index.js"
    },
    "./create-server-context": {
      "types": "./dist/cjs/create-server-context.d.ts",
      "import": "./dist/esm/create-server-context.js",
      "require": "./dist/cjs/create-server-context.js",
      "default": "./dist/cjs/create-server-context.js"
    },
    "./get-server-context": {
      "types": "./dist/cjs/get-server-context.d.ts",
      "import": "./dist/esm/get-server-context.js",
      "require": "./dist/cjs/get-server-context.js",
      "default": "./dist/cjs/get-server-context.js"
    }
  },
  "scripts": {
    "build": "tsc && tsc --project tsconfig.esm.json"
  },
  "keywords": [
    "next",
    "next.js",
    "server components",
    "react.js",
    "implementation",
    "rsc",
    "server context",
    "context",
    "getter"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/alexdln/nimpl-context.git"
  },
  "author": {
    "name": "Alex Savelyev",
    "email": "dev@alexdln.com",
    "url": "https://github.com/alexdln/"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/node": "24.10.1",
    "@types/react": "19.2.7",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "next": "16.0.7",
    "typescript": "5.9.3"
  },
  "peerDependencies": {
    "react": ">= 18.2.0",
    "react-dom": ">= 18.2.0",
    "next": ">= 14.0.0"
  }
}